{
  "name": "tourze/symfony-aop-cache-bundle",
  "description": "AOP缓存支持",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-redis": "*",
    "psr/cache": "^2.0 || ^3.0",
    "symfony/cache": "^6.4",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/var-exporter": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/backtrace-helper": "~0.0.5",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/symfony-aop-bundle": "~0.0.4",
    "tourze/symfony-cache-hotkey-bundle": "0.0.*",
    "tourze/symfony-cron-job-bundle": "0.1.*",
    "twig/twig": "^3.13|^4.0"
  },
  "require-dev": {
    "maglnet/composer-require-checker": "^4",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopCacheBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopCacheBundle\\Tests\\": "tests"
    }
  }
}