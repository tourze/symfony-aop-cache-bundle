{
  "name": "tourze/symfony-aop-cache-bundle",
  "description": "缓存支持",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "^6.4",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/backtrace-helper": "~0.0.5",
    "tourze/symfony-cache-hotkey-bundle": "0.0.*",
    "tourze/symfony-cron-job-bundle": "0.0.*",
    "tourze/symfony-aop-bundle": "0.0.*",
    "twig/twig": "^3.13|^4.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopCacheBundle\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}